{
  "name": "root",
  "version": "0.0.0",
  "packageManager": "yarn@4.0.2",
  "private": true,
  "type": "module",
  "workspaces": [
    "app",
    "db",
    "edge",
    "scripts",
    "server"
  ],
  "scripts": {
    "postinstall": "husky install && node ./scripts/post-install.js",
    "start": "yarn workspace app start",
    "lint": "eslint --cache --report-unused-disable-directives .",
    "test": "vitest",
    "build": "yarn workspaces foreach -tiA run build",
    "deploy": "yarn workspace edge deploy"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/eslint-plugin": "^11.11.0",
    "@types/eslint": "^8.56.1",
    "@types/react-datepicker": "^6",
    "@types/react-helmet": "^6",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "graphql": "^16.8.1",
    "happy-dom": "^13.0.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.35",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "relay-config": "^12.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.3",
    "vite": "~5.0.11",
    "vitest": "~1.1.3"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "dependencies": {
    "@mui/x-date-pickers": "^7.2.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@react-login-page/page9": "^1.0.4",
    "@welcome-ui/emoji-picker": "^5.13.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "embla-carousel-react": "^8.0.0",
    "html2canvas": "^1.4.1",
    "init": "^0.1.2",
    "jspdf": "^2.5.1",
    "lucide-react": "^0.336.0",
    "node-fetch": "^3.3.2",
    "picmo": "^5.8.5",
    "react-bootstrap": "^2.10.1",
    "react-datepicker": "^6.9.0",
    "react-helmet": "^6.1.0",
    "react-login-page": "^1.0.4",
    "shadcn-ui": "^0.8.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  }
}
